{
  "extends": [
    "config:base",
    ":semanticCommitType(fix)"
  ],
  "branchPrefix": "develop-renovate-",
  "prCreation": "not-pending",
  "labels": [
    "maintenance"
  ],
  "constraints": {
    "npm": "^7.0.0"
  },
  "packageRules": [
    {
      "updateTypes": ["major"],
      "automerge": false
    },
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "packageNames": ["webpack", "react-scripts"],
      "groupName": "create-react-app"
    }
  ]
}
